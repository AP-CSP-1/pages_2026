    "<h2>üéÆ Loop Control</h2>\n",
    "\n",
    "<div class=\"js-code\">\n",
    "// break = exit loop immediately\n",
    "for (let i = 1; i <= 10; i++) {\n",
    "    if (i === 5) break;  // Stops at 5\n",
    "    console.log(i);      // Prints: 1, 2, 3, 4\n",
    "}\n",
    "\n",
    "// continue = skip to next iteration\n",
    "for (let i = 1; i <= 5; i++) {\n",
    "    if (i === 3) continue;  // Skips 3\n",
    "    console.log(i);         // Prints: 1, 2, 4, 5\n",
    "}\n",
    "</div>\n",
